<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <OutputType>WinExe</OutputType>
        <LangVersion>preview</LangVersion>
        <AnalysisLevel>preview</AnalysisLevel>
        <UseWindowsForms>true</UseWindowsForms>
        <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
        <TargetFrameworks>net48;net481;net6.0-windows;net7.0-windows;net8.0-windows;net9.0-windows</TargetFrameworks>
    </PropertyGroup>
    <PropertyGroup>
        <ApplicationIcon>Resources\ICO.ico</ApplicationIcon>
        <StartupObject>WinFormsDemoWallpaper.Program</StartupObject>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Skylark" Version="3.1.3.4" />
        <PackageReference Include="Skylark.Wing" Version="3.1.3.7" />
    </ItemGroup>
    <ItemGroup>
      <Compile Update="DuplicateWall.cs">
        <SubType>Form</SubType>
      </Compile>
    </ItemGroup>
</Project>